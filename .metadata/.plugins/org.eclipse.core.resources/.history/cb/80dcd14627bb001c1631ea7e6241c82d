public class Auto {
	public static void main(String args[])
	{
		boolean array[] = {false,false,true,true};
		System.out.println("Java boolean Array Example");
		for(int i=0;i < array.length;i++)
		{
			System.out.println("boolean array Element at : "+ i + " " + array[i]);
		}
	}
}